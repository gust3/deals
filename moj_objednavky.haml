                  .steps2
                    %h3 Registration
                    .line
                    %form#form-registr{:action => "#"}
                      %label{:for => "input-email"}
                        Email
                        %sup *
                      %input#input-email{:type => "text"}/
                      %br.clear/
                      .tabs You must enter your email address in the correct format. For example username@domain.ru.
                      %br.clear/
                      %fieldset.fields-password
                        %label{:for => "input-password"}
                          Password
                          %sup *
                        %input#input-password{:type => "password"}/
                        .eye.closed-eye
                        %br.clear/
                        %label.label-again{:for => "input-again"}
                          Once again
                          %sup *
                        %input#input-again{:type => "password"}/
                      %label.label-fullname{:for => "input-fullname"} Full name
                      %input#input-fullname{:type => "text"}/
                      %br.clear/
