                      %input#input-telephone{:type => "text"}/
                      %input#input-telephone-code{:type => "text"}/
                      %br.clear/
                      %span.span-code Code
                      %span.span-number Number
                      %br.clear/
                      %label.label-adress Adress
                      %fieldset.adress-items
                        %label{:for => "input-country"} Country
                        %select#input-country{:type => "text"}
                          %option Значение 1
                          %option Значение 2
                          %option Значение 3
                          %option Значение 4
                          %option Значение 5
                        %br.clear/
                        %label{:for => "input-city"} City
                        %select#input-city{:type => "text"}
                          %option Значение 1
                          %option Значение 2
                          %option Значение 3
                          %option Значение 4
                          %option Значение 5
                        %br.clear/
                        %label{:for => "yourcity"} Your city
                        %input#yourcity{:type => "text"}/
                        %br.clear/
                        .attention
                          %span Attention!
                          If your city is not listed, choose the closest to your city from the list, and the name of your locality specify in the box below.
                        %br.clear/
                        %label{:for => "zipcode"} ZIP code
                        %input#zipcode{:type => "text"}/
                      %br.clear/
                      .agree
                        %input#checkagr{:type => "checkbox"}/
                        %span By registering, you agree
                        %a{:href => "#"} to the terms of use.
                        %button.register_normal
